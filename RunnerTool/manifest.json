{
    "name": "Corsa Tools",
    "manifest_version": 2,
    "description": "Utilities for the keen runner",
    "default_locale": "it",
    "version": "5.2.1.0",
    "app": {
        "launch": {
            "local_path": "toolrun.html"
        }
    },
    "icons": {
        "16": "Counter_16.png",
        "24": "Counter_24.png",
        "48": "Counter_48.png",
        "128": "Counter_128.png"
    },
    "browser_action": {
        "default_title": "Strumenti della corsa",
        "default_icon": "Counter_48.png"
    },
    "permissions": [
        "unlimited_storage",
        "notifications"
    ]
}